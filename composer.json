{
  "name": "xlthlx/com",
  "license": "MIT",
  "description": "Theme based on Timber and Bootstrap 5.",
  "type": "wordpress-theme",
  "minimum-stability": "stable",
  "authors": [
    {
      "email": "xlthlx@gmail.com",
      "name": "xlthlx"
    }
  ],
  "require": {
    "cmb2/cmb2": "^v2.10.0",
    "composer/installers": "^1.12",
    "dejurin/php-google-translate-for-free": "^1.0",
    "scrivo/highlight.php": "dev-master",
    "simplehtmldom/simplehtmldom": "dev-master",
    "johnbillion/extended-cpts": "^5.0",
    "timber/timber": "^1.19"
  },
  "extra": {
    "installer-paths": {
      "inc/{$vendor}/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "require-dev": {
    "dxw/phar-install": "^1.1"
  },
  "scripts": {
    "post-update-cmd": "vendor/bin/phar-install"
  }
